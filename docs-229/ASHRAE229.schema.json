{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "ASHRAE 229",
    "description": "Base schema for ASHRAE 229 representations",
    "definitions": {
        "ASHRAE229": {
            "type": "object",
            "properties": {
                "transformers": {
                    "description": "Electrical transformers at the building site",
                    "type": "array",
                    "minItems": 1,
                    "items": {
                        "$ref": "ASHRAE229.schema.json#/definitions/Transformer"
                    },
                    "notes": "Contains a list of transformers that convert electricity from a higher voltage to one used by the building, exterior lighting, and other services at the site."
                },
                "buildings": {
                    "description": "Buildings on the site",
                    "type": "array",
                    "minItems": 1,
                    "items": {
                        "$ref": "ASHRAE229.schema.json#/definitions/Building"
                    },
                    "notes": "Contains a list of buildings on the site (often just one)."
                },
                "calendar": {
                    "description": "Information on the calendar used with the simulation.",
                    "$ref": "ASHRAE229.schema.json#/definitions/Calendar"
                },
                "schedules": {
                    "description": "Schedules for internal loads, thermostats, equipment operation and control, and any other need.",
                    "type": "array",
                    "minItems": 1,
                    "items": {
                        "$ref": "ASHRAE229.schema.json#/definitions/Schedule"
                    },
                    "notes": "Contains a list of schedules used in model."
                },
                "weather": {
                    "description": "Information on the local weather conditions used with the simulation.",
                    "$ref": "ASHRAE229.schema.json#/definitions/Weather"
                },
                "overall_simulation_outputs": {
                    "description": "Outputs from the simluation summed for all buildings in the simulation.",
                    "$ref": "ASHRAE229.schema.json#/definitions/OverallSimulationOutputs"
                }
            },
            "additionalProperties": false
        },
        "Building": {
            "type": "object",
            "properties": {
                "id": {
                    "description": "Unique Identification Number",
                    "type": "number",
                    "notes": ""
                },
                "name": {
                    "description": "Name of the Building",
                    "type": "string",
                    "notes": ""
                },
                "number_of_floors": {
                    "description": "Number of floors",
                    "type": "number",
                    "minimum": 0.0,
                    "notes": ""
                },
                "building_segments": {
                    "description": "Large portions of a building that share a building area type",
                    "type": "array",
                    "minItems": 1,
                    "items": {
                        "$ref": "ASHRAE229.schema.json#/definitions/BuildingSegment"
                    },
                    "notes": "Contains a list of building segments in the building."
                },
                "is_new": {
                    "description": "The schedules assume it is a leap year",
                    "type": "boolean"
                },
                "elevators": {
                    "description": "Elevators",
                    "type": "array",
                    "minItems": 1,
                    "items": {
                        "$ref": "ASHRAE229.schema.json#/definitions/Elevator"
                    },
                    "notes": "Contains a list of elevators in the building."
                },
                "refrigeration_components": {
                    "description": "Refrigeration",
                    "type": "array",
                    "minItems": 1,
                    "items": {
                        "$ref": "ASHRAE229.schema.json#/definitions/Refrigeration"
                    },
                    "notes": "Contains a list of refrigeration components in the building."
                }
            },
            "required": [
                "id",
                "name",
                "number_of_floors",
                "is_new"
            ],
            "additionalProperties": false
        },
        "BuildingSegment": {
            "type": "object",
            "properties": {
                "id": {
                    "description": "Unique Identification Number",
                    "type": "number",
                    "notes": ""
                },
                "thermal_blocks": {
                    "description": "thermal blocks in the building",
                    "type": "array",
                    "minItems": 1,
                    "items": {
                        "$ref": "ASHRAE229.schema.json#/definitions/ThermalBlock"
                    },
                    "notes": "Contains a list of thermal blocks in the building."
                },
                "heating_ventilation_air_conditioning_systems": {
                    "description": "HVAC systems in the building",
                    "type": "array",
                    "minItems": 1,
                    "items": {
                        "$ref": "ASHRAE229.schema.json#/definitions/HeatingVentilationAirConditioningSystem"
                    },
                    "notes": "Contains a list of HVAC systems in the building."
                },
                "area_type_vertical_fenestration": {
                    "description": "Building area classification used for vertical fenestration",
                    "$ref": "Enumerations2019ASHRAE901.schema.json#/definitions/VerticalFenestrationBuildingAreaType2019ASHRAE901",
                    "notes": "The enumeration is based on the standard used."
                }
            },
            "required": [
                "id",
                "area_type_vertical_fenestration"
            ],
            "additionalProperties": false
        },
        "ThermalBlock": {
            "type": "object",
            "properties": {
                "zones": {
                    "description": "zones in the building",
                    "type": "array",
                    "minItems": 1,
                    "items": {
                        "$ref": "ASHRAE229.schema.json#/definitions/Zone"
                    },
                    "notes": "Contains a list of zones in the building."
                },
                "served_by_heating_ventilation_air_conditioning_systems": {
                    "description": "HVAC systems serving the thermal block",
                    "type": "array",
                    "minItems": 1,
                    "items": {
                        "type": "string"
                    },
                    "notes": "Contains a list of IDs of the HVAC systems serving the thermal block - from Unique Identification Number in HeatingVentilationAirConditioningSystem."
                }
            },
            "additionalProperties": false
        },
        "Zone": {
            "type": "object",
            "properties": {
                "spaces": {
                    "description": "Spaces in the building",
                    "type": "array",
                    "minItems": 1,
                    "items": {
                        "$ref": "ASHRAE229.schema.json#/definitions/Space"
                    },
                    "notes": "Contains a list of spaces in the building."
                }
            },
            "additionalProperties": false
        },
        "Space": {
            "type": "object",
            "properties": {
                "id": {
                    "description": "Unique Identification Number",
                    "type": "number",
                    "notes": ""
                },
                "name": {
                    "description": "Name fo the Space",
                    "type": "string",
                    "notes": ""
                },
                "surfaces": {
                    "description": "Surfaces surrounding the space",
                    "type": "array",
                    "minItems": 1,
                    "items": {
                        "$ref": "ASHRAE229.schema.json#/definitions/Surface"
                    },
                    "notes": "Contains a list of surfaces that define the space."
                },
                "floor_area": {
                    "description": "The floor area of a space within the building, including basements, mezzanine and intermediate-floored tiers, and penthouses with a headroom height of 7.5 ft or greater. It is measured from the exterior faces of walls or from the center-line of walls separating buildings, but excluding covered walkways, open roofed-over areas, porches and similar spaces, pipe trenches, exterior terraces or steps, chimneys, roof overhangs, and similar features.",
                    "type": "number",
                    "minimum": 0.0,
                    "units": "m2",
                    "notes": ""
                },
                "floor_area_basis_for_exterior": {
                    "description": "The basis of the measurement location related to floor area for exterior walls.",
                    "$ref": "ASHRAE229.schema.json#/definitions/FloorAreaBasis",
                    "notes": ""
                },
                "floor_area_basis_for_interior": {
                    "description": "The basis of the measurement location related to floor area for interior walls.",
                    "$ref": "ASHRAE229.schema.json#/definitions/FloorAreaBasis",
                    "notes": ""
                },
                "conditioning_type": {
                    "description": "Space conditioning category",
                    "$ref": "ASHRAE229.schema.json#/definitions/ConditioningType",
                    "notes": ""
                },
                "lighting_space_type": {
                    "description": "Lighting space type classification",
                    "$ref": "Enumerations2019ASHRAE901.schema.json#/definitions/LightingSpaceType2019ASHRAE901",
                    "notes": "The enumeration is based on the standard used."
                },
                "ventilations_space_type": {
                    "description": "Ventilation space type classification",
                    "$ref": "Enumerations2019ASHRAE901.schema.json#/definitions/VentilationSpaceType2019ASHRAE901",
                    "notes": "The enumeration is based on the standard used."
                },
                "infiltration_modeling_method": {
                    "description": "The software methodology chosen for modeling infiltration",
                    "type": "string"
                },
                "infiltration_equivalent_full_load_hours": {
                    "description": "Annual sum of hourly fractions of infiltration schedule",
                    "type": "number",
                    "minimum": 0.0
                }
            },
            "required": [
                "id",
                "name",
                "floor_area",
                "floor_area_basis_for_exterior",
                "floor_area_basis_for_interior",
                "conditioning_type",
                "lighting_space_type",
                "ventilations_space_type",
                "infiltration_modeling_method",
                "infiltration_equivalent_full_load_hours"
            ],
            "additionalProperties": false
        },
        "FloorAreaBasis": {
            "type": "string",
            "enum": [
                "CENTER",
                "NEAR_SIDE",
                "FAR_SIDE"
            ],
            "enum_text": [
                "The area is measured to the center line of the wall",
                "The area is measured to the line on the near side wall",
                "The area is measured to the line on the far side wall"
            ],
            "descriptions": [
                "Center line of the wall",
                "Near side of the wall",
                "Far side of the wall"
            ]
        },
        "ConditioningType": {
            "type": "string",
            "enum": [
                "HEATED_AND_COOLED",
                "HEATED_ONLY",
                "SEMIHEATED",
                "UNCONDITIONED",
                "PLENUM"
            ],
            "enum_text": [
                "Heated and cooled",
                "Heated only",
                "Semiheated",
                "Unconditioned",
                "Plenum"
            ],
            "descriptions": [
                "Heated and cooled",
                "Heated only",
                "Semiheated",
                "Unconditioned",
                "Plenum"
            ]
        },
        "Surface": {
            "type": "object",
            "properties": {
                "id": {
                    "description": "Unique Identification Number",
                    "type": "number",
                    "notes": ""
                },
                "name": {
                    "description": "Name fo the Space",
                    "type": "string",
                    "notes": ""
                },
                "fenestration_subsurfaces": {
                    "description": "Fenestration suburfaces that are on the surface",
                    "type": "array",
                    "minItems": 1,
                    "items": {
                        "$ref": "ASHRAE229.schema.json#/definitions/Fenestration"
                    },
                    "notes": "Contains a list of surfaces that define the space."
                },
                "tilt": {
                    "description": "Angle between vertical and the surface outward normal, e.g. 0 = roof, 90 = wall, 180 = downward facing surface (exterior floor)",
                    "type": "number",
                    "units": "degrees"
                },
                "azimuth": {
                    "description": "Clockwise angle between North (0 degrees) and the horizontal projection of the wall's outward normal. 0 = north, 90 = East, 180 = South, 270 = West",
                    "type": "number",
                    "minimum": 0.0,
                    "units": "degrees"
                },
                "adjacent_to": {
                    "description": "Determines whether this is an (a) exterior surface if adjacent to ambient, (b) slab-on-grad or below grade surface if adjacent to ground, or (c) interior surface if adjacent to another thermal block.",
                    "$ref": "ASHRAE229.schema.json#/definitions/SurfaceAdjacentTo"
                },
                "adjacent_space_ID": {
                    "description": "ID of the adjacent space for interior surface",
                    "type": "string"
                },
                "does_cast_shade": {
                    "description": "Determines whether the surface is modeled as casting shade on other exterior surfaces",
                    "type": "boolean"
                },
                "surfaces": {
                    "description": "Surfaces surrounding the space",
                    "type": "array",
                    "minItems": 1,
                    "items": {
                        "$ref": "ASHRAE229.schema.json#/definitions/Surface"
                    },
                    "notes": "Contains a list of surfaces that define the space."
                },
                "surface_construction_input_option": {
                    "description": "Identifies whether construction is entered layer-by-layer or simplified (R-value)",
                    "$ref": "ASHRAE229.schema.json#/definitions/SurfaceConstructionInputOptions"
                },
                "area": {
                    "description": "area of the surface",
                    "type": "number",
                    "minimum": 0.0,
                    "units": "m2"
                },
                "u_factor": {
                    "description": "suface U-factor",
                    "type": "number",
                    "minimum": 0.0,
                    "units": "W/m2-K"
                },
                "c_factor": {
                    "description": "surface C-factor",
                    "type": "number",
                    "minimum": 0.0
                },
                "f_factor": {
                    "description": "surface F-factor",
                    "type": "number",
                    "minimum": 0.0
                },
                "reflectance": {
                    "description": "Reflectance",
                    "type": "number",
                    "minimum": 0.0
                },
                "emittance": {
                    "description": "Emittance",
                    "type": "number",
                    "minimum": 0.0
                },
                "reflectivity": {
                    "description": "Reflectivity",
                    "type": "number",
                    "minimum": 0.0
                }
            },
            "required": [
                "id",
                "name",
                "tilt",
                "azimuth",
                "adjacent_to",
                "adjacent_space_ID",
                "does_cast_shade",
                "surface_construction_input_option",
                "area",
                "u_factor",
                "c_factor",
                "f_factor",
                "reflectance",
                "emittance",
                "reflectivity"
            ],
            "additionalProperties": false
        },
        "SurfaceAdjacentTo": {
            "type": "string",
            "enum": [
                "AMBIENT",
                "GROUND",
                "INTERIOR"
            ],
            "descriptions": [
                "Exterior wall or roof which is adjacent to the exterior ambient environments.",
                "Slab-on-grad or below grade surface if adjacent to ground.",
                "Interior surface if adjacent to another thermal block."
            ]
        },
        "SurfaceConstructionInputOptions": {
            "type": "string",
            "enum": [
                "LAYERS",
                "SIMPLIFIED"
            ],
            "descriptions": [
                "Construction is entered layer-by-layer.",
                "Construction is entered by R-value only."
            ]
        },
        "Fenestration": {
            "type": "object",
            "properties": {
                "id": {
                    "description": "Unique Identification Number",
                    "type": "number",
                    "notes": ""
                },
                "name": {
                    "description": "Name of the fenestration subsurface",
                    "type": "string",
                    "notes": ""
                },
                "area": {
                    "description": "area of fenestration including glass and framing",
                    "type": "number",
                    "minimum": 0.0,
                    "units": "m2"
                },
                "u_factor": {
                    "description": "fenestration U-factor",
                    "type": "number",
                    "minimum": 0.0,
                    "units": "W/m2-K"
                },
                "solar_heat_gain_coefficient": {
                    "description": "fenestration SHGC",
                    "type": "number",
                    "minimum": 0.0
                },
                "has_shading_projections": {
                    "description": "identifies whether fenestration has side fins, overhangs or not flush with wall",
                    "type": "boolean"
                },
                "has_manual_interior_shades": {
                    "description": "are there manually-operated interior shading such as blinds, curtains or shades",
                    "type": "boolean"
                },
                "has_automatic_shades": {
                    "description": "are there automatic interior shading such as blinds, curtains or shades",
                    "type": "boolean"
                }
            },
            "required": [
                "id",
                "name",
                "area",
                "u_factor",
                "solar_heat_gain_coefficient",
                "has_shading_projections",
                "has_manual_interior_shades",
                "has_automatic_shades"
            ],
            "additionalProperties": false
        },
        "Transformer": {
            "type": "object",
            "properties": {
                "name": {
                    "description": "Transformer Name",
                    "type": "string",
                    "notes": ""
                },
                "type": {
                    "description": "The type of transformer",
                    "$ref": "ASHRAE229.schema.json#/definitions/TransformerType",
                    "notes": ""
                },
                "phase": {
                    "description": "The number of electrical phases",
                    "$ref": "ASHRAE229.schema.json#/definitions/TransformerPhase",
                    "notes": ""
                },
                "efficiency": {
                    "description": "Transformer efficiency",
                    "type": "number",
                    "minimum": 0.0,
                    "notes": ""
                },
                "capacity": {
                    "description": "Rated Capacity of the Transformer",
                    "type": "number",
                    "minimum": 0.0,
                    "units": "Va",
                    "notes": ""
                },
                "peak_load": {
                    "description": "Annual Peak electric load on the transformer",
                    "type": "number",
                    "minimum": 0.0,
                    "units": "W",
                    "notes": ""
                },
                "capacity_ratio": {
                    "description": "Annual Peak electric load of the transformer divided by the capacity",
                    "type": "number",
                    "minimum": 0.0,
                    "notes": ""
                }
            },
            "required": [
                "name",
                "type",
                "phase",
                "efficiency",
                "capacity",
                "peak_load",
                "capacity_ratio"
            ],
            "additionalProperties": false
        },
        "TransformerType": {
            "type": "string",
            "enum": [
                "DRY_TYPE",
                "FLUID_FILLED",
                "OTHER"
            ],
            "enum_text": [
                "Dry Type",
                "Fluid Filled",
                "Other"
            ],
            "descriptions": [
                "Dry Type",
                "Fluid Filled",
                "Other"
            ]
        },
        "TransformerPhase": {
            "type": "string",
            "enum": [
                "SINGLE_PHASE",
                "THREE_PHASE"
            ],
            "enum_text": [
                "Single Phase",
                "Three Phase"
            ],
            "descriptions": [
                "Single Phase",
                "Three Phase"
            ]
        },
        "Schedule": {
            "type": "object",
            "properties": {
                "id": {
                    "description": "Unique Identification Number",
                    "type": "number",
                    "notes": ""
                },
                "name": {
                    "description": "Name of the Schedule",
                    "type": "string",
                    "notes": ""
                },
                "type": {
                    "description": "The type of schedule",
                    "type": "string",
                    "notes": "Not an enumerations because we only care that the type assigned by BEM tool matches across RMR"
                },
                "values": {
                    "description": "Hourly Values of Schedule",
                    "type": "array",
                    "minItems": 1,
                    "maxItems": 8760,
                    "items": {
                        "type": "number"
                    },
                    "notes": ""
                }
            },
            "required": [
                "id",
                "name",
                "type",
                "values"
            ],
            "additionalProperties": false
        },
        "Calendar": {
            "type": "object",
            "properties": {
                "id": {
                    "description": "Unique Identification Number",
                    "type": "number"
                },
                "day_of_week_for_january_1": {
                    "description": "Day of the Week for January 1",
                    "$ref": "ASHRAE229.schema.json#/definitions/DayOfWeek"
                },
                "is_leap_year": {
                    "description": "The schedules assume it is a leap year",
                    "type": "boolean"
                },
                "is_daylight_savings_time": {
                    "description": "The schedules adjust for daylight Savings Time",
                    "type": "boolean"
                }
            },
            "required": [
                "id",
                "day_of_week_for_january_1",
                "is_leap_year",
                "is_daylight_savings_time"
            ],
            "additionalProperties": false
        },
        "DayOfWeek": {
            "type": "string",
            "enum": [
                "SUNDAY",
                "MONDAY",
                "TUESDAY",
                "WEDNESDAY",
                "THURSDAY",
                "FRIDAY",
                "SATURDAY"
            ],
            "descriptions": [
                "Sunday",
                "Monday",
                "Tuesday",
                "Wednesday",
                "Thursday",
                "Friday",
                "Saturday"
            ]
        },
        "Weather": {
            "type": "object",
            "properties": {
                "monthly_ground_temperature": {
                    "description": "Modeled monthly ground temperatures",
                    "type": "array",
                    "minItems": 1,
                    "maxItems": 12,
                    "items": {
                        "type": "number"
                    },
                    "units": "C"
                },
                "climate_zone": {
                    "description": "The designation of the climate zone where the building is located",
                    "$ref": "Enumerations2019ASHRAE901.schema.json#/definitions/ClimateZone2019ASHRAE901",
                    "notes": "The enumeration is based on the standard used."
                }
            },
            "required": [
                "monthly_ground_temperature",
                "climate_zone"
            ],
            "additionalProperties": false
        },
        "Elevator": {
            "type": "object",
            "properties": {
                "id": {
                    "description": "Unique Identification Number",
                    "type": "number",
                    "notes": ""
                },
                "name": {
                    "description": "Name of the elevator",
                    "type": "string",
                    "notes": ""
                },
                "motor_power": {
                    "description": "Elevator peak motor power",
                    "type": "number",
                    "units": "W"
                },
                "cab_counterweight": {
                    "description": "elevator car counterweight",
                    "type": "number",
                    "units": "kg"
                },
                "cab_weight": {
                    "description": "weight of elevator car",
                    "type": "number",
                    "units": "kg"
                },
                "design_elevator_load": {
                    "description": "elevator load at which to operate",
                    "type": "number",
                    "units": "kg"
                },
                "speed": {
                    "description": "design speed of the elevator",
                    "type": "number",
                    "units": "m/s"
                },
                "cab_area": {
                    "description": "floor area of elevator cab",
                    "type": "number",
                    "units": "m2"
                },
                "cab_lighting_power": {
                    "description": "lighitng power of cab",
                    "type": "number",
                    "units": "W"
                },
                "cab_ventilation_fan_power": {
                    "description": "ventilation fan power of cab",
                    "type": "number",
                    "units": "W"
                },
                "cab_ventilation_fan_flow": {
                    "description": "airflow of cab ventfan",
                    "type": "number",
                    "units": "L/s"
                },
                "cab_motor_schedule": {
                    "description": "elevator motor operation schedule name",
                    "type": "string"
                },
                "cab_ventilation_fan_schedule": {
                    "description": "elevator vventfan operation schedule name",
                    "type": "string"
                },
                "cab_lighting_schedule": {
                    "description": "elevator lighting schedule name",
                    "type": "string"
                },
                "cab_motor_schedule_equivalent_full_load_hours": {
                    "description": "elevator motor operation schedule equivalent full load hours",
                    "type": "number",
                    "minimum": 0.0,
                    "maximum": 8760.0
                },
                "cab_ventilation_fan_schedule_equivalent_full_load_hours": {
                    "description": "elevator ventfan operation schedule equivalent full load hours",
                    "type": "number",
                    "minimum": 0.0,
                    "maximum": 8760.0
                },
                "cab_lighting_schedule_equivalent_full_load_hours": {
                    "description": "elevator lighitng schedule equivalent full load hours",
                    "type": "number",
                    "minimum": 0.0,
                    "maximum": 8760.0
                }
            },
            "required": [
                "id",
                "name",
                "motor_power",
                "cab_counterweight",
                "cab_weight",
                "design_elevator_load",
                "speed",
                "cab_area",
                "cab_lighting_power",
                "cab_ventilation_fan_power",
                "cab_ventilation_fan_flow",
                "cab_motor_schedule",
                "cab_ventilation_fan_schedule",
                "cab_lighting_schedule",
                "cab_motor_schedule_equivalent_full_load_hours",
                "cab_ventilation_fan_schedule_equivalent_full_load_hours",
                "cab_lighting_schedule_equivalent_full_load_hours"
            ],
            "additionalProperties": false
        },
        "HeatingVentilationAirConditioningSystem": {
            "type": "object",
            "properties": {
                "id": {
                    "description": "Unique Identification Number",
                    "type": "number"
                },
                "sensible_cool_output_capacity": {
                    "description": "Result from the simulation of the sensible cooling capacity",
                    "type": "number",
                    "minimum": 0.0,
                    "units": "W/m2"
                },
                "heat_output_capacity": {
                    "description": "Result from the simulation of the heating capacity",
                    "type": "number",
                    "minimum": 0.0,
                    "units": "W/m2"
                }
            },
            "required": [
                "id",
                "sensible_cool_output_capacity",
                "heat_output_capacity"
            ],
            "additionalProperties": false
        },
        "Refrigeration": {
            "type": "object",
