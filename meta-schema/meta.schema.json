{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ASHRAE Standard 205 Meta-schema",
  "definitions": {
    "ObjectType": {
      "type": "string",
      "enum": [
        "Meta",
        "Data Type",
        "String Type",
        "Enumeration",
        "Data Group",
        "Rating Data Group",
        "Performance Map",
        "Grid Variables",
        "Lookup Variables"
      ]
    },
    "Meta": {
      "type": "object",
      "properties": {
        "Object Type": {
          "$ref": "meta.schema.json#/definitions/ObjectType"
        },
        "Title": {
          "type": "string"
        },
        "Description": {
          "type": "string"
        },
        "Version": {
          "type": "string"
        },
        "Root Data Group": {
          "type": "string"
        },
        "References": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      },
      "required": [
        "Object Type"
      ],
      "additionalProperties": false
    },
    "DataType": {
      "type": "object",
      "properties": {
        "Object Type": {
          "$ref": "meta.schema.json#/definitions/ObjectType"
        },
        "Description": {
          "type": "string"
        },
        "JSON Schema Type": {
          "type": "string",
          "enum": [
            "string",
            "number",
            "integer",
            "boolean",
            "null"
          ]
        },
        "Examples": {
          "type": "array"
        }
      },
      "required": [
        "Object Type",
        "Description",
        "JSON Schema Type",
        "Examples"
      ],
      "additionalProperties": false
    },
    "StringType": {
      "type": "object",
      "properties": {
        "Object Type": {
          "$ref": "meta.schema.json#/definitions/ObjectType"
        },
        "Description": {
          "type": "string"
        },
        "JSON Schema Pattern": {
          "type": "string"
        },
        "Examples": {
          "type": "array"
        },
        "Is Regex": {
          "type": "boolean"
        }
      },
      "required": [
        "Object Type",
        "Description",
        "Examples"
      ],
      "additionalProperties": false
    },
    "Enumerator": {
      "type": "object",
      "patternProperties": {
        "(^([A-Z]+([A-Z]|[0-9])*)(_([A-Z]*|[0-9]*))*$)": {
          "type": [
            "object",
            "null"
          ],
          "properties": {
            "Description": {
              "type": "string"
            },
            "Display Text": {
              "type": "string"
            },
            "Notes": {
              "type": "string"
            }
          },
          "required": [],
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    },
    "Enumeration": {
      "type": "object",
      "properties": {
        "Object Type": {
          "$ref": "meta.schema.json#/definitions/ObjectType"
        },
        "Enumerators": {
          "$ref": "meta.schema.json#/definitions/Enumerator"
        }
      },
      "required": [
        "Object Type",
        "Enumerators"
      ],
      "additionalProperties": false
    },
    "StandardUnits": {
      "type": "string",
      "enum": [
        "-",
        "m",
        "K",
        "W",
        "kg",
        "kg/s",
        "m3/s",
        "Pa",
        "m2-K/W",
        "Hz",
        "V",
        "rev/s",
        "Va",
        "m2",
        "L/s",
        "m/s",
        "W/m2",
        "degrees",
        "W/m2-K",
        "W/m-K",
        "K-m2/W",
        "C",
        "kWh",
        "m3",
        "hr",
        "J"
      ]
    },
    "RatingUnits": {
      "type": "string",
      "enum": [
        "-",
        "m",
        "K",
        "W",
        "kg",
        "kg/s",
        "m3/s",
        "Pa",
        "m2-K/W",
        "F",
        "gpm",
        "ft of water",
        "Btu/h",
        "Btu/W-h",
        "W/(m3/s)",
        "W/cfm",
        "%",
        "h-ft2-F/Btu",
        "Hz",
        "V",
        "C",
        "kW",
        "m2-K/kW",
        "kPa",
        "l/s",
        "rev/s"
      ]
    },
    "DataTypePattern": {
      "type": "string",
      "pattern": "((Numeric|String|Integer|Boolean|Null){1}(\\/(Numeric|String|Integer|Boolean|Null))*|(UUID|Date|Timestamp|Version|Pattern)|\\{[A-Z]([A-Z]|[a-z]|[0-9])*(\\(([A-Z]*|[a-z]*)(_([a-z]*|[0-9]*)|([A-Z]*|[0-9]*))*=(.*)\\))?\\}|<[A-Z]([A-Z]|[a-z]|[0-9])*>)|\\(((Numeric|String|Integer|Boolean|Null){1}(\\/(Numeric|String|Integer|Boolean|Null))*|(UUID|Date|Timestamp|Version|Pattern)|\\{[A-Z]([A-Z]|[a-z]|[0-9])*(\\(([A-Z]*|[a-z]*)(_([a-z]*|[0-9]*)|([A-Z]*|[0-9]*))*=(.*)\\))?\\}|<[A-Z]([A-Z]|[a-z]|[0-9])*>)(,\\s*((Numeric|String|Integer|Boolean|Null){1}(\\/(Numeric|String|Integer|Boolean|Null))*|(UUID|Date|Timestamp|Version|Pattern)|\\{[A-Z]([A-Z]|[a-z]|[0-9])*(\\(([A-Z]*|[a-z]*)(_([a-z]*|[0-9]*)|([A-Z]*|[0-9]*))*=(.*)\\))?\\}|<[A-Z]([A-Z]|[a-z]|[0-9])*>))+\\)|\\[((Numeric|String|Integer|Boolean|Null){1}(\\/(Numeric|String|Integer|Boolean|Null))*|(UUID|Date|Timestamp|Version|Pattern)|\\{[A-Z]([A-Z]|[a-z]|[0-9])*(\\(([A-Z]*|[a-z]*)(_([a-z]*|[0-9]*)|([A-Z]*|[0-9]*))*=(.*)\\))?\\}|<[A-Z]([A-Z]|[a-z]|[0-9])*>)\\]"
    },
    "DataElementAttributes": {
      "type": "object",
      "properties": {
        "Description": {
          "type": "string"
        },
        "Data Type": {
          "$ref": "meta.schema.json#/definitions/DataTypePattern"
        },
        "Units": {
          "$ref": "meta.schema.json#/definitions/StandardUnits"
        },
        "Range": {
          "type": "string"
        },
        "Required": {
          "type": "boolean"
        },
        "Notes": {
          "type": "string"
        }
      }
    },
    "RatingDataElementAttributes": {
      "type": "object",
      "properties": {
        "Description": {
          "type": "string"
        },
        "Data Type": {
          "$ref": "meta.schema.json#/definitions/DataTypePattern"
        },
        "Units": {
          "$ref": "meta.schema.json#/definitions/RatingUnits"
        },
        "Range": {
          "type": "string"
        },
        "Required": {
          "type": "boolean"
        },
        "Notes": {
          "type": "string"
        }
      }
    },
    "GridVariableDataElementAttributes": {
      "type": "object",
      "properties": {
        "Description": {
          "type": "string"
        },
        "Data Type": {
          "type": "string",
          "enum": [
            "[Numeric][1..]",
            "[Integer][1..]"
          ]
        },
        "Units": {
          "$ref": "meta.schema.json#/definitions/StandardUnits"
        },
        "Range": {
          "type": "string"
        },
        "Required": {
          "type": "boolean",
          "const": true
        },
        "Notes": {
          "type": "string"
        }
      }
    },
    "LookupVariableDataElementAttributes": {
      "type": "object",
      "properties": {
        "Description": {
          "type": "string"
        },
        "Data Type": {
          "type": "string",
          "enum": [
            "[Numeric/Null][1..]",
            "[Integer/Null][1..]"
          ]
        },
        "Units": {
          "$ref": "meta.schema.json#/definitions/StandardUnits"
        },
        "Range": {
          "type": "string"
        },
        "Required": {
          "type": "boolean",
          "const": true
        },
        "Notes": {
          "type": "string"
        }
      }
    },
    "DataElement": {
      "type": "object",
      "patternProperties": {
        "(^([A-Z]*|[a-z]*)(_([a-z]*|[0-9]*)|([A-Z]*|[0-9]*))*$)": {
          "$ref": "meta.schema.json#/definitions/DataElementAttributes",
          "required": [
            "Description",
            "Data Type"
          ],
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    },
    "RatingDataElement": {
      "type": "object",
      "patternProperties": {
        "(^([A-Z]*|[a-z]*)(_([a-z]*|[0-9]*)|([A-Z]*|[0-9]*))*$)": {
          "$ref": "meta.schema.json#/definitions/RatingDataElementAttributes",
          "required": [
            "Description",
            "Data Type"
          ],
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    },
    "GridVariableDataElement": {
      "type": "object",
      "patternProperties": {
        "(^([A-Z]*|[a-z]*)(_([a-z]*|[0-9]*)|([A-Z]*|[0-9]*))*$)": {
          "$ref": "meta.schema.json#/definitions/GridVariableDataElementAttributes",
          "required": [
            "Description",
            "Data Type",
            "Units",
            "Required"
          ],
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    },
    "LookupVariableDataElement": {
      "type": "object",
      "patternProperties": {
        "(^([A-Z]*|[a-z]*)(_([a-z]*|[0-9]*)|([A-Z]*|[0-9]*))*$)": {
          "$ref": "meta.schema.json#/definitions/LookupVariableDataElementAttributes",
          "required": [
            "Description",
            "Data Type",
            "Units",
            "Required"
          ],
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    },
    "DataGroup": {
      "type": "object",
      "properties": {
        "Object Type": {
          "$ref": "meta.schema.json#/definitions/ObjectType"
        },
        "Data Elements": {
          "$ref": "meta.schema.json#/definitions/DataElement"
        }
      },
      "required": [
        "Object Type",
        "Data Elements"
      ],
      "additionalProperties": false
    },
    "RatingDataGroup": {
      "type": "object",
      "properties": {
        "Object Type": {
          "$ref": "meta.schema.json#/definitions/ObjectType"
        },
        "Data Elements": {
          "$ref": "meta.schema.json#/definitions/RatingDataElement",
          "required": [
            "rating_reproducible_from_performance_data"
          ]
        }
      },
      "required": [
        "Object Type",
        "Data Elements"
      ],
      "additionalProperties": false
    },
    "PerformanceMap": {
      "type": "object",
      "properties": {
        "Object Type": {
          "$ref": "meta.schema.json#/definitions/ObjectType"
        },
        "Data Elements": {
          "type": "object",
          "properties": {
            "grid_variables": {
              "$ref": "meta.schema.json#/definitions/DataElementAttributes",
              "required": [
                "Description",
                "Data Type",
                "Required"
              ],
              "additionalProperties": false
            },
            "lookup_variables": {
              "$ref": "meta.schema.json#/definitions/DataElementAttributes",
              "required": [
                "Description",
                "Data Type",
                "Required"
              ],
              "additionalProperties": false
            }
          },
          "required": [
            "grid_variables",
            "lookup_variables"
          ],
          "additionalProperties": false
        }
      },
      "required": [
        "Object Type",
        "Data Elements"
      ],
      "additionalProperties": false
    },
    "GridVariables": {
      "type": "object",
      "properties": {
        "Object Type": {
          "$ref": "meta.schema.json#/definitions/ObjectType"
        },
        "Data Elements": {
          "$ref": "meta.schema.json#/definitions/GridVariableDataElement"
        }
      },
      "required": [
        "Object Type",
        "Data Elements"
      ],
      "additionalProperties": false
    },
    "LookupVariables": {
      "type": "object",
      "properties": {
        "Object Type": {
          "$ref": "meta.schema.json#/definitions/ObjectType"
        },
        "Data Elements": {
          "$ref": "meta.schema.json#/definitions/LookupVariableDataElement"
        }
      },
      "required": [
        "Object Type",
        "Data Elements"
      ],
      "additionalProperties": false
    }
  },
  "type": "object",
  "patternProperties": {
    "(^[A-Z]([A-Z]|[a-z]|[0-9])*$)": {
      "allOf": [
        {
          "if": {
            "properties": {
              "Object Type": {
                "const": "Meta"
              }
            }
          },
          "then": {
            "$ref": "meta.schema.json#/definitions/Meta"
          }
        },
        {
          "if": {
            "properties": {
              "Object Type": {
                "const": "Data Type"
              }
            }
          },
          "then": {
            "$ref": "meta.schema.json#/definitions/DataType"
          }
        },
        {
          "if": {
            "properties": {
              "Object Type": {
                "const": "String Type"
              }
            }
          },
          "then": {
            "$ref": "meta.schema.json#/definitions/StringType"
          }
        },
        {
          "if": {
            "properties": {
              "Object Type": {
                "const": "Enumeration"
              }
            }
          },
          "then": {
            "$ref": "meta.schema.json#/definitions/Enumeration"
          }
        },
        {
          "if": {
            "properties": {
              "Object Type": {
                "const": "Data Group"
              }
            }
          },
          "then": {
            "$ref": "meta.schema.json#/definitions/DataGroup"
          }
        },
        {
          "if": {
            "properties": {
              "Object Type": {
                "const": "Rating Data Group"
              }
            }
          },
          "then": {
            "$ref": "meta.schema.json#/definitions/RatingDataGroup"
          }
        },
        {
          "if": {
            "properties": {
              "Object Type": {
                "const": "Performance Map"
              }
            }
          },
          "then": {
            "$ref": "meta.schema.json#/definitions/PerformanceMap"
          }
        },
        {
          "if": {
            "properties": {
              "Object Type": {
                "const": "Map Variables"
              }
            }
          },
          "then": {
            "$ref": "meta.schema.json#/definitions/MapVariables"
          }
        },
        {
          "if": {
            "properties": {
              "Object Type": {
                "const": "Grid Variables"
              }
            }
          },
          "then": {
            "$ref": "meta.schema.json#/definitions/GridVariables"
          }
        },
        {
          "if": {
            "properties": {
              "Object Type": {
                "const": "Lookup Variables"
              }
            }
          },
          "then": {
            "$ref": "meta.schema.json#/definitions/LookupVariables"
          }
        },
        {
          "type": "object",
          "properties": {
            "Object Type": {
              "$ref": "meta.schema.json#/definitions/ObjectType"
            }
          },
          "required": [
            "Object Type"
          ]
        }
      ]
    }
  },
  "additionalProperties": false
}